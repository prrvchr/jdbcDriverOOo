      </node>


      <node oor:name="MetaData">

        <node oor:name="CreateTableCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>CREATE ${TableType} ${TableName} (${ColumnDescriptions})${Versioning}</value>
          </prop>
        </node>
        <node oor:name="DropTableCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>DROP TABLE ${TableName}</value>
          </prop>
        </node>
        <node oor:name="CreateViewCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>CREATE VIEW ${ViewName} AS ${SelectCommand}</value>
          </prop>
        </node>
        <node oor:name="DropViewCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>DROP VIEW ${Name}</value>
          </prop>
        </node>
        <node oor:name="AlterViewCommands" oor:op="replace">
          <prop oor:name="Value" oor:type="oor:string-list">
            <value oor:separator="|">ALTER VIEW ${ViewName} AS ${SelectCommand}</value>
          </prop>
        </node>
        <node oor:name="AddColumnCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>ALTER TABLE ${TableName} ADD COLUMN ${ColumnDescription}</value>
          </prop>
        </node>
        <node oor:name="AddPrimaryKeyCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>ALTER TABLE ${TableName} ADD PRIMARY KEY (${Columns})</value>
          </prop>
        </node>
        <node oor:name="AddForeignKeyCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>ALTER TABLE ${TableName} ADD FOREIGN KEY (${Columns}) REFERENCES ${ReferencedTable} (${RelatedColumns}) ${UpdateRule} ${DeleteRule}</value>
          </prop>
        </node>
        <node oor:name="AddUniqueConstraintCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>ALTER TABLE ${TableName} ADD CONSTRAINT ${Name} UNIQUE (${Columns})</value>
          </prop>
        </node>
        <node oor:name="CreateIndexCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>CREATE INDEX ${Name} ON ${TableName} (${Columns})</value>
          </prop>
        </node>
        <node oor:name="DropIndexCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>DROP INDEX ${Name}</value>
          </prop>
        </node>
        <node oor:name="DropPrimaryKeyCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>ALTER TABLE ${TableName} DROP PRIMARY KEY</value>
          </prop>
        </node>
        <node oor:name="DropConstraintCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>ALTER TABLE ${TableName} DROP CONSTRAINT ${Name}</value>
          </prop>
        </node>
        <node oor:name="AlterColumnCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>ALTER TABLE ${TableName} ALTER COLUMN ${ColumnDescription}</value>
          </prop>
        </node>
        <node oor:name="DropColumnCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>ALTER TABLE ${TableName} DROP COLUMN ${Column}</value>
          </prop>
        </node>
        <node oor:name="AlterColumnNameCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>ALTER TABLE ${TableName} ALTER COLUMN ${OldName} RENAME TO ${Column}</value>
          </prop>
        </node>
        <node oor:name="ColumnSetNotNullCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>ALTER TABLE ${TableName} ALTER COLUMN ${Column} SET NOT NULL</value>
          </prop>
        </node>
        <node oor:name="ColumnDropNotNullCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>ALTER TABLE ${TableName} ALTER COLUMN ${Column} DROP NOT NULL</value>
          </prop>
        </node>
        <node oor:name="ColumnSetDefaultCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>ALTER TABLE ${TableName} ALTER COLUMN ${Column} SET DEFAULT ${Default}</value>
          </prop>
        </node>
        <node oor:name="ColumnDropDefaultCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>ALTER TABLE ${TableName} ALTER COLUMN ${Column} DROP DEFAULT</value>
          </prop>
        </node>
        <node oor:name="ColumnDropIdentityCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>ALTER TABLE ${TableName} ALTER COLUMN ${Column} DROP IDENTITY</value>
          </prop>
        </node>
        <node oor:name="TableDescriptionCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>COMMENT ON TABLE ${TableName} IS ${Description}</value>
          </prop>
        </node>
        <node oor:name="ColumnDescriptionCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>COMMENT ON COLUMN ${ColumnName} IS ${Description}</value>
          </prop>
        </node>

        <node oor:name="CreateUserCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>CREATE USER ${User} PASSWORD ${Password}</value>
          </prop>
        </node>
        <node oor:name="DropUserCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>DROP USER ${User}</value>
          </prop>
        </node>
        <node oor:name="AlterUserCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>ALTER USER ${User} SET PASSWORD ${Password}</value>
          </prop>
        </node>
        <node oor:name="CreateRoleCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>CREATE ROLE ${Role}</value>
          </prop>
        </node>
        <node oor:name="DropRoleCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>DROP ROLE ${Role}</value>
          </prop>
        </node>
        <node oor:name="GrantRoleCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>GRANT ${Grantor} TO ${Grantee}</value>
          </prop>
        </node>
        <node oor:name="RevokeRoleCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>REVOKE ${Grantor} FROM ${Grantee}</value>
          </prop>
        </node>
        <node oor:name="GrantPrivilegesCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>GRANT ${Privileges} ON ${TableName} TO ${Grantee}</value>
          </prop>
        </node>
        <node oor:name="RevokePrivilegesCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>REVOKE ${Privileges} ON ${TableName} FROM ${Grantee}</value>
          </prop>
        </node>

        <node oor:name="LogLevel" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>OFF</value>
          </prop>
        </node>
        <node oor:name="UseJava" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:boolean">
            <value>true</value>
          </prop>
        </node>
        <node oor:name="Authentication" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>UserPassword</value>
          </prop>
        </node>
        <node oor:name="SupportsTableCreation" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:boolean">
            <value>true</value>
          </prop>
        </node>
      </node>
    </node>

<!-- HsqlDB Driver ********************************************************************************* -->

    <node oor:name="xdbc:hsqldb:*" oor:op="replace">
      <prop oor:name="ParentURLPattern">
        <value>xdbc:*</value>
      </prop>
      <prop oor:name="DriverTypeDisplayName" oor:type="xs:string">
        <value xml:lang="en-US">Pure Java HsqlDB</value>
        <value xml:lang="fr-FR">Pur Java HsqlDB</value>
      </prop>

      <!-- Properties node is for connection properties -->
      <!-- To be accessible by Base, the properties must be listed in the DriverBase.getPropertyInfo() method. -->
      <node oor:name="Properties">
        <node oor:name="TypeInfoSettings" oor:op="replace">
          <prop oor:name="Value" oor:type="oor:string-list">
            <value oor:separator="|">Column(1) = TIME|Column(6) = SCALE|Column(1) = TIME|Column(14) = 0|Column(1) = TIME|Column(15) = 6|Column(1) = TIMESTAMP|Column(6) = SCALE|Column(1) = TIMESTAMP|Column(14) = 0|Column(1) = TIMESTAMP|Column(15) = 6|Column(1) = TIME WITH TIME ZONE|Column(6) = SCALE|Column(1) = TIME WITH TIME ZONE|Column(14) = 0|Column(1) = TIME WITH TIME ZONE|Column(15) = 6|Column(1) = TIMESTAMP WITH TIME ZONE|Column(6) = SCALE|Column(1) = TIMESTAMP WITH TIME ZONE|Column(12) = true|Column(1) = TIMESTAMP WITH TIME ZONE|Column(14) = 0|Column(1) = TIMESTAMP WITH TIME ZONE|Column(15) = 6</value>
          </prop>
        </node>
        <node oor:name="AddIndexAppendix" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:boolean">
            <value>true</value>
          </prop>
        </node>
        <node oor:name="JavaDriverClassPath" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>%origin%/driver/hsqldb.jar</value>
          </prop>
        </node>
        <node oor:name="JavaDriverClass" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>org.hsqldb.jdbcDriver</value>
          </prop>
        </node>
        <node oor:name="IsAutoRetrievingEnabled" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:boolean">
            <value>true</value>
          </prop>
        </node>
        <node oor:name="AutoIncrementCreation" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>GENERATED BY DEFAULT AS IDENTITY</value>
          </prop>
        </node>
        <node oor:name="RowVersionCreation" oor:op="replace">
          <prop oor:name="Value" oor:type="oor:string-list">
            <value oor:separator="|">GENERATED ALWAYS AS ROW START|GENERATED ALWAYS AS ROW END</value>
          </prop>
        </node>
        <node oor:name="AutoRetrievingStatement" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>SELECT * FROM %s WHERE %s</value>
          </prop>
        </node>
        <node oor:name="InMemoryDataBase" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>hsqldb:mem:dbversion</value>
          </prop>
        </node>
        <node oor:name="IgnoreDriverPrivileges" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:boolean">
            <value>false</value>
          </prop>
        </node>
        <node oor:name="TablePrivilegesSettings" oor:op="replace">
          <prop oor:name="Value" oor:type="oor:string-list">
            <value oor:separator="|">SELECT|1|INSERT|2|UPDATE|4|DELETE|8|REFERENCES|128</value>
          </prop>
        </node>
      </node>

      <!-- MetaData node is for the driver properties -->
      <node oor:name="MetaData">
        <node oor:name="UseSQLInsert" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:boolean">
            <value>true</value>
          </prop>
        </node>
        <node oor:name="UpdateVisibleInsensitive" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:boolean">
            <value>false</value>
          </prop>
        </node>
        <node oor:name="AutoIncrementIsPrimaryKey" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:boolean">
            <value>false</value>
          </prop>
        </node>
        <node oor:name="DropTableCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>DROP TABLE ${TableName} CASCADE</value>
          </prop>
        </node>
