          <prop oor:name="Value" oor:type="xs:string">
            <value>SET PASSWORD FOR ${User} = PASSWORD(${Password})</value>
          </prop>
        </node>
        <node oor:name="CreateUserCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>CREATE USER ${User} IDENTIFIED BY ${Password}</value>
          </prop>
        </node>
        <node oor:name="GetUsersQuery" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>SELECT User FROM mysql.user WHERE host = '%' AND LENGTH(authentication_string)</value>
          </prop>
        </node>
        <node oor:name="GetGroupsQuery" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>SELECT User FROM mysql.user WHERE host = '%' AND NOT LENGTH(authentication_string)</value>
          </prop>
        </node>
        <node oor:name="GetUserGroupsQuery" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
          <!-- XXX: We use recursion to find privileges inherited from roles.
             XXX: We need to filter recursive entries (even role and user) -->
            <value>SELECT r.FROM_USER FROM mysql.role_edges r INNER JOIN mysql.user u ON r.FROM_USER = u.User AND u.host = '%' AND NOT LENGTH(u.authentication_string) WHERE r.TO_USER = ${User}</value>
          </prop>
        </node>
        <node oor:name="GetGroupUsersQuery" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>SELECT r.TO_USER FROM mysql.role_edges r INNER JOIN mysql.user u ON r.TO_USER = u.User AND u.host = '%' AND LENGTH(u.authentication_string) WHERE r.FROM_USER = ${Group}</value>
          </prop>
        </node>
        <node oor:name="GetGroupRolesQuery" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>SELECT r.FROM_USER FROM mysql.role_edges r INNER JOIN mysql.user u ON r.FROM_USER = u.User AND u.host = '%' AND NOT LENGTH(u.authentication_string) WHERE r.TO_USER = ${Group}</value>
          </prop>
        </node>
        <node oor:name="TablePrivilegesQuery" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>SELECT CASE WHEN LOCATE('Select', Table_priv) THEN 1 ELSE 0 END, CASE WHEN LOCATE('Insert', Table_priv) THEN 2 ELSE 0 END, CASE WHEN LOCATE('Update', Table_priv) THEN 4 ELSE 0 END, CASE WHEN LOCATE('Delete', Table_priv) AND (LOCATE('Delete', Table_priv) != LOCATE('Delete versioning rows', Table_priv) OR NOT LOCATE('Delete versioning rows', Table_priv)) THEN 8 ELSE 0 END, CASE WHEN LOCATE('Delete versioning rows', Table_priv) THEN 32 ELSE 0 END, CASE WHEN LOCATE('Alter', Table_priv) THEN 64 ELSE 0 END, CASE WHEN LOCATE('Index', Table_priv) THEN 128 ELSE 0 END, CASE WHEN LOCATE('Drop', Table_priv) THEN 256 ELSE 0 END, CASE WHEN LOCATE('Trigger', Table_priv) THEN 512 ELSE 0 END FROM mysql.tables_priv WHERE User = ${Grantee} AND Db = ${Catalog} AND Table_name = ${Table}</value>
          </prop>
        </node>
      </node>
    </node>

