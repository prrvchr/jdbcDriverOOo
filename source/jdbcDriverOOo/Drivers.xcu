          </prop>
        </node>
        <node oor:name="IgnoreCurrency" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:boolean">
            <value>true</value>
          </prop>
        </node>
        <node oor:name="PrivilegesSettings" oor:op="replace">
          <prop oor:name="Value" oor:type="oor:string-list">
            <value oor:separator="|">SELECT|1|INSERT|2|UPDATE|4|DELETE|8|REFERENCES|128</value>
          </prop>
        </node>
      </node>

      <node oor:name="MetaData">
        <node oor:name="SupportTransaction" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:boolean">
            <value>false</value>
          </prop>
        </node>
        <node oor:name="AutoIncrementIsPrimaryKey" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:boolean">
            <value>false</value>
          </prop>
        </node>
        <node oor:name="SupportsColumnDescription" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:boolean">
            <value>true</value>
          </prop>
        </node>
        <node oor:name="SupportsRenameView" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:boolean">
            <value>false</value>
          </prop>
        </node>
        <node oor:name="AddColumnCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>ALTER TABLE ${TableName} ADD ${ColumnDescription}</value>
          </prop>
        </node>
        <node oor:name="DropColumnCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>ALTER TABLE ${TableName} DROP ${Column}</value>
          </prop>
        </node>
        <node oor:name="AddPrimaryKeyCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>ALTER TABLE ${TableName} ADD CONSTRAINT ${Name} PRIMARY KEY (${Columns})</value>
          </prop>
        </node>
        <node oor:name="DropPrimaryKeyCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>ALTER TABLE ${TableName} DROP CONSTRAINT ${Name}</value>
          </prop>
        </node>
        <node oor:name="GrantRoleCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>GRANT ${Grantor} TO ${RoleType} ${Grantee}</value>
          </prop>
        </node>
        <node oor:name="RevokeRoleCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>REVOKE ${Grantor} FROM ${RoleType} ${Grantee}</value>
          </prop>
        </node>
        <node oor:name="GrantPrivilegesCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>GRANT ${Privileges} ON ${TableName} TO ${RoleType} ${Grantee}</value>
          </prop>
        </node>
        <node oor:name="RevokePrivilegesCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>REVOKE ${Privileges} ON ${TableName} FROM ${RoleType} ${Grantee}</value>
          </prop>
        </node>
        <node oor:name="AlterViewCommands" oor:op="replace">
          <prop oor:name="Value" oor:type="oor:string-list">
            <value oor:separator="|">DROP VIEW ${ViewName}|CREATE VIEW ${ViewName} AS ${SelectCommand}</value>
          </prop>
        </node>
        <node oor:name="ViewDefinitionQuery" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>SELECT RDB$VIEW_SOURCE FROM RDB$RELATIONS WHERE RDB$RELATION_NAME = ${RawView}</value>
          </prop>
        </node>
        <node oor:name="AlterColumnTypeCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>ALTER TABLE ${TableName} ALTER COLUMN ${Column} TYPE ${Type}</value>
          </prop>
        </node>
        <node oor:name="AlterColumnNameCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>ALTER TABLE ${TableName} ALTER COLUMN ${Column} TO ${NewColumn}</value>
          </prop>
        </node>
        <node oor:name="GetUsersQuery" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>SELECT TRIM(SEC$USER_NAME) FROM SEC$USERS</value>
          </prop>
        </node>
        <node oor:name="GetGroupsQuery" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>SELECT TRIM(RDB$ROLE_NAME) FROM RDB$ROLES</value>
          </prop>
        </node>
        <node oor:name="GetUserGroupsQuery" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>SELECT TRIM(P.RDB$RELATION_NAME) FROM RDB$USER_PRIVILEGES AS P INNER JOIN SEC$USERS AS U ON TRIM(P.RDB$USER) = TRIM(U.SEC$USER_NAME) WHERE TRIM(P.RDB$PRIVILEGE) = 'M' AND P.RDB$USER_TYPE = 8 AND P.RDB$OBJECT_TYPE = 13 AND TRIM(P.RDB$USER) = ${User}</value>
          </prop>
        </node>
        <node oor:name="GetGroupUsersQuery" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>SELECT TRIM(P.RDB$USER) FROM RDB$USER_PRIVILEGES AS P INNER JOIN SEC$USERS AS U ON TRIM(P.RDB$USER) = TRIM(U.SEC$USER_NAME) WHERE TRIM(P.RDB$PRIVILEGE) = 'M' AND P.RDB$USER_TYPE = 8 AND P.RDB$OBJECT_TYPE = 13 AND TRIM(P.RDB$RELATION_NAME) = ${Group}</value>
          </prop>
        </node>
        <node oor:name="GetGroupRolesQuery" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>SELECT TRIM(P.RDB$USER) FROM RDB$USER_PRIVILEGES AS P INNER JOIN RDB$ROLES AS R ON TRIM(P.RDB$USER) = TRIM(R.RDB$ROLE_NAME) WHERE TRIM(P.RDB$PRIVILEGE) = 'M' AND P.RDB$USER_TYPE = 8 AND P.RDB$OBJECT_TYPE = 13 AND TRIM(P.RDB$RELATION_NAME) = ${Group}</value>
          </prop>
        </node>
        <node oor:name="TablePrivilegesQuery" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>SELECT CASE TRIM(RDB$PRIVILEGE) WHEN 'S' THEN 1 WHEN 'I' THEN 2 WHEN 'U' THEN 4 WHEN 'D' THEN 8 WHEN 'R' THEN 128 END FROM RDB$USER_PRIVILEGES WHERE RDB$OBJECT_TYPE = 0 AND TRIM(RDB$USER) = ${Grantee} AND TRIM(RDB$RELATION_NAME) = ${Table}</value>
          </prop>
        </node>
      </node>
    </node>

  </node>
