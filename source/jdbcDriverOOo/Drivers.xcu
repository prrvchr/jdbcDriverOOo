            <value>true</value>
          </prop>
        </node>
        <node oor:name="ColumnDescriptionCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>IF EXISTS(SELECT value FROM fn_listextendedproperty('ColumnDescription', 'SCHEMA', '${RawSchema}', 'TABLE', '${RawTable}', 'COLUMN', '${RawColumn}')) EXECUTE sp_updateextendedproperty @name = 'ColumnDescription', @level0type = 'SCHEMA', @level1type = N'TABLE', @level2type = N'COLUMN', @level0name=N'${RawSchema}', @level1name=N'${RawTable}', @level2name = N'${RawColumn}', @value = '${RawDescription}' ELSE EXECUTE sp_addextendedproperty @name = 'ColumnDescription', @level0type = 'SCHEMA', @level1type = N'TABLE', @level2type = N'COLUMN', @level0name=N'${RawSchema}', @level1name=N'${RawTable}', @level2name = N'${RawColumn}', @value = '${RawDescription}'</value>
          </prop>
        </node>
        <node oor:name="ColumnDescriptionQuery" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>SELECT value FROM fn_listextendedproperty('ColumnDescription', 'SCHEMA', '${RawSchema}', 'TABLE', '${RawTable}', 'COLUMN', '${RawColumn}')</value>
          </prop>
        </node>
        <node oor:name="ViewDefinitionQuery" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>SELECT SUBSTRING(VIEW_DEFINITION, (CHARINDEX(' AS SELECT ', VIEW_DEFINITION) + 4)) FROM INFORMATION_SCHEMA.VIEWS WHERE TABLE_SCHEMA = ${RawSchema} AND TABLE_NAME = ${RawView}</value>
          </prop>
        </node>
        <node oor:name="RenameTableCommands" oor:op="replace">
          <prop oor:name="Value" oor:type="oor:string-list">
            <value oor:separator="|">EXEC sp_rename '${RawTableName}', '${RawNewTable}'</value>
          </prop>
        </node>
        <node oor:name="SupportsAlterIdentity" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:boolean">
            <value>true</value>
          </prop>
        </node>
        <node oor:name="GetUsersQuery" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>SELECT name FROM sys.server_principals WHERE type = 'S' AND is_disabled = 0</value>
          </prop>
        </node>
        <node oor:name="GetGroupsQuery" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>sp_helprole</value>
          </prop>
        </node>
        <node oor:name="GetUserGroupsQuery" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>SELECT DP1.name FROM sys.database_role_members AS DRM RIGHT OUTER JOIN sys.database_principals AS DP1 ON DRM.role_principal_id = DP1.principal_id LEFT OUTER JOIN sys.database_principals AS DP2 ON DRM.member_principal_id = DP2.principal_id WHERE DP1.type = 'R' AND DP2.name = ${User}</value>
          </prop>
        </node>
        <node oor:name="GetGroupUsersQuery" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>SELECT DP1.name FROM sys.database_role_members AS DRM RIGHT OUTER JOIN sys.database_principals AS DP1 ON DRM.member_principal_id = DP1.principal_id LEFT OUTER JOIN sys.database_principals AS DP2 ON DRM.role_principal_id = DP2.principal_id WHERE DP1.type = 'S' AND DP2.name = ${Group}</value>
          </prop>
        </node>
        <node oor:name="GetGroupRolesQuery" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>SELECT DP1.name FROM sys.database_role_members AS DRM RIGHT OUTER JOIN sys.database_principals AS DP1 ON DRM.role_principal_id = DP1.principal_id LEFT OUTER JOIN sys.database_principals AS DP2 ON DRM.member_principal_id = DP2.principal_id WHERE DP1.type = 'R' AND DP2.name = ${Group}</value>
          </prop>
        </node>
        <node oor:name="TablePrivilegesQuery" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>SELECT permission_name FROM sys.database_permissions DPE INNER JOIN sys.database_principals DPR on DPE.grantee_principal_id = DPR.principal_id WHERE DPE.class = 1 AND DPE.major_id = object_id(${TableName}) and DPE.minor_id = 0 AND DPR.name = ${Grantee}</value>
          </prop>
        </node>
        <node oor:name="GrantablePrivilegesQuery" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>SELECT permission_name FROM sys.database_permissions DPE INNER JOIN sys.database_principals DPR on DPE.grantee_principal_id = DPR.principal_id WHERE DPE.class = 1 AND DPE.major_id = object_id(${TableName}) and DPE.minor_id = 0 AND DPR.name = ${Grantee}</value>
          </prop>
        </node>
        <node oor:name="CreateUserCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>CREATE LOGIN ${RawUser} WITH PASSWORD = ${Password}; CREATE USER ${RawUser} FOR LOGIN ${RawUser}</value>
          </prop>
        </node>
        <node oor:name="AlterUserCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>ALTER LOGIN ${RawUser} WITH PASSWORD = ${Password}</value>
          </prop>
        </node>
        <node oor:name="CreateRoleCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>CREATE ROLE ${RawRole}</value>
          </prop>
        </node>
        <node oor:name="DropRoleCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>DROP ROLE ${RawRole}</value>
          </prop>
        </node>
        <node oor:name="GrantRoleCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>ALTER ROLE ${Grantor} ADD MEMBER ${Grantee}</value>
          </prop>
        </node>
        <node oor:name="RevokeRoleCommand" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>ALTER ROLE ${Grantor} DROP MEMBER ${Grantee}</value>
          </prop>
        </node>
      </node>
    </node>

<!-- Trino Driver ******************************************************************************* -->

    <node oor:name="xdbc:trino:*" oor:op="replace">
