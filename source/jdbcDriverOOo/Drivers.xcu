          </prop>
        </node>
        <node oor:name="GetUserGroupsQuery" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>SELECT ROLEID FROM SYS.SYSROLES WHERE ISDEF = 'N' AND ROLEID != GRANTEE AND GRANTEE = ${User}</value>
          </prop>
        </node>
        <node oor:name="GetGroupUsersQuery" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>SELECT GRANTEE FROM SYS.SYSROLES WHERE ISDEF = 'N' AND GRANTEE != ROLEID AND ROLEID = ${Group}</value>
          </prop>
        </node>
        <node oor:name="TablePrivilegesQuery" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>SELECT CASE WHEN P.SELECTPRIV = 'y' THEN 1 ELSE 0 END, CASE WHEN P.INSERTPRIV = 'y' THEN 2 ELSE 0 END, CASE WHEN P.UPDATEPRIV = 'y' THEN 4 ELSE 0 END, CASE WHEN P.DELETEPRIV = 'y' THEN 8 ELSE 0 END, CASE WHEN P.REFERENCESPRIV = 'y' THEN 128 ELSE 0 END, CASE WHEN P.TRIGGERPRIV = 'y' THEN 512 ELSE 0 END FROM SYS.SYSTABLEPERMS AS P INNER JOIN SYS.SYSTABLES AS T ON P.TABLEID = T.TABLEID INNER JOIN SYS.SYSSCHEMAS AS S ON T.SCHEMAID = S.SCHEMAID WHERE P.GRANTEE = ${Grantee} AND S.SCHEMANAME = ${Schema} AND T.TABLENAME = ${Table}</value>
