          <prop oor:name="Value" oor:type="xs:string">
            <value>io.github.prrvchr.jdbcDriverOOo.DefinitionContainer</value>
          </prop>
        </node>
        <node oor:name="Reports" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:string">
            <value>io.github.prrvchr.jdbcdriver.sdb.DocumentContainer</value>
          </prop>
        </node> -->
      </node>

<!-- "Features" node is the properties that are supposed to be exposed in the UI in: Base -> Edit -> DataBase -> Advanced settings -->
      <node oor:name="Features">
        <node oor:name="UseDOSLineEnds" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:boolean">
            <value>true</value>
          </prop>
        </node>
        <node oor:name="FormsCheckRequiredFields" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:boolean">
            <value>true</value>
          </prop>
        </node>
        <node oor:name="EscapeDateTime" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:boolean">
            <value>true</value>
          </prop>
        </node>
        <!-- Corresponds to GenerateASBeforeCorrelationName in Properties - see connectivity/registry/README -->
        <node oor:name="UseKeywordAsBeforeAlias" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:boolean">
            <value>true</value>
          </prop>
        </node>
        <node oor:name="GeneratedValues" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:boolean">
            <value>true</value>
          </prop>
        </node>
        <node oor:name="RespectDriverResultSetType" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:boolean">
            <value>true</value>
          </prop>
        </node>
        <node oor:name="PrimaryKeySupport" oor:op="replace">
          <prop oor:name="Value" oor:type="xs:boolean">
